{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "catagories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catagory.catagory",
      "inversedBy": "products"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "inversedBy": "products"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "colors": {
      "type": "json"
    },
    "stocks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stock.stock"
    }
  }
}
